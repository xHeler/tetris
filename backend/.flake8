[flake8]
max-line-length = 127
max-complexity = 11
exclude=
    */migrations/*
    __pycache__
    manage.py
    settings.py
    .venv
    templates
    .*
    README.mode
    requirements.text
    docker-compose.yml
    static
    .vscode
per-file-ignores =
    ./random_file.py:E501
    ./settings/local.py:F405,F403
    ./settings/prod.py:F405,F403,F401
    ./settings/staging.py:F405,F403
    ./accounts/apps.py:F40